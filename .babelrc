{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "node": 6,
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "transform-object-rest-spread"
    ],
    [
      "syntax-dynamic-import"
    ],
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      }
    ]
  ]
}